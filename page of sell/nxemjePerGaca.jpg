</div><div id="shopify-section-template--15320060035262__main-collection" class="shopify-section"><script type="text/javascript">
  document.addEventListener('page:loaded', function() {
    const sidebarContainer = document.querySelector(".grid__item--sidebar");
    const sidebar = document.getElementById("CollectionSidebar");
    const header = document.querySelector(".header-section");
    const gridBottom = document.getElementById("grid-bottom");
    let lastPaddingTop = 0
    function getOffset( el ) {
        var _x = 0;
        var _y = 0;
        while( el && !isNaN( el.offsetLeft ) && !isNaN( el.offsetTop ) ) {
            _x += el.offsetLeft - el.scrollLeft;
            _y += el.offsetTop - el.scrollTop;
            el = el.offsetParent;
        }
        return { top: _y, left: _x };
    }

    const fixedHeader = () => {
      var y = window.scrollY
      //console.log(y);
      const height = header.clientHeight
      //const max = grid.clientHeight + grid.offsetTop - sidebar.clientHeight
      const change = sidebarContainer.offsetTop - height - 20
      let m = getOffset(gridBottom).top
      const max = m - sidebar.clientHeight * 2

      if (y > change) {
        if (y < max) {
          lastPaddingTop = (header.clientHeight + 20);
          sidebar.style.position = "fixed";
          sidebar.style.top = lastPaddingTop + "px";
          sidebar.style.opacity = 1
        } else {
          lastPaddingTop = (header.clientHeight + 20);
          sidebar.style.position = "fixed";
          sidebar.style.top = lastPaddingTop + "px";
          sidebar.style.opacity = 0
        }
        /*} else {
          sidebar.style.paddingTop = lastPaddingTop + "px"
        }*/
      } else {
        sidebar.style.position = "inherit";
        sidebar.style.top = 0;
      }
    }

    if (sidebarContainer && sidebar && header) {
      fixedHeader();
      document.addEventListener('scroll', function(e) {
          fixedHeader();
      });
    }
  });
</script>

